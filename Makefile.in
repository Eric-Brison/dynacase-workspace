PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir=@PUBRULE@
appname = @PACKAGE@
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best

export targetdir PACKAGE

SUBDIR=  Class Actions Zone Css Images 

include $(utildir)/PubRule

DISTFILES += $(SUBDIR) \
            RELEASE VERSION   workspace.csv



$(pubdir)/$(appname)/workspace.csv : workspace.csv 
	cp workspace.csv $(pubdir)/$(appname)

publish: $(pubdir)/$(appname)/workspace.csv