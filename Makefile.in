PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir=@PUBRULE@
appname = @PACKAGE@
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best

export targetdir PACKAGE

SUBDIR=  Class Actions Zone Css Images 

include $(utildir)/PubRule

DISTFILES += $(SUBDIR) \
            RELEASE VERSION   workspace.ods



$(pubdir)/$(appname)/workspace.ods : workspace.ods
	cp workspace.ods $(pubdir)/$(appname)

publish: $(pubdir)/$(appname)/workspace.ods