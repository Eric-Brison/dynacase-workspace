  <!--
    [JS:CODE] 
   var idbasket='[FREEDOM_IDBASKET]';

function expandtree(oimg,id,ulid,adddocid,padddocid,addft) {
  
  var r=viewfoldertree(oimg,id,document.getElementById(ulid),adddocid,padddocid,addft);
  if (r==1) {
    oimg.src='[IMG:b_down.png]'; 
  } else if (r==0) {
    oimg.src='[IMG:b_right.png]';
  } else if (r==2) {
    oimg.src='[IMG:b_wait.png]';
  }
  if (isIE) correctOnePNG(oimg);
}
function expandToptree(o,id,ulid) {

  var ldiv=o.parentNode.getElementsByTagName("div");
  for (var i=0;i<ldiv.length;i++) {
    if (ldiv[i].className=='spaceselect') ldiv[i].className='space';
  }
  o.className="spaceselect";
  CURSPACE=o;
  var r=viewfoldertree(null,id,ulid);
  
  ulid.innerHTML='<table style="width:100%;height:80%"><tr><td align="center"><img style="width:48px" onload="if (isIE) correctOnePNG(this)" src="[IMG:b_wait.png]"></tr></td></table>';
}
function clipboardWait(o) {
  if (o) o.innerHTML='<table style="width:100%;height:100%"><tr><td align="center"><img style="width:48px" onload="if (isIE) correctOnePNG(this)" src="[IMG:b_wait.png]"></tr></td></table>';
}

   function endexpandtree(o,c) {
     if (o) {
       if (c==0) {
	 o.src='[IMG:none.png]'; 
	 o.style.visibility='hidden';
	 if (isIE) correctOnePNG(o);
       }
       else {
	 o.src='[IMG:b_down.png]'; 
	 o.style.visibility='visible';
       }
     }
   }
  //-->
