<html>
  <head>

   <title>[TEXT:Documents exchange]</title>

<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
<link rel="shortcut icon" href="FDL/Images/logo16.ico" >

[CSS:REF]


   <style type="text/css">
[CSS:CODE]
   </style>

[JS:REF]

<script type="text/javascript" src="[APPNAME]/Layout/displayws.js"></script>
<script type="text/javascript" src="[APPNAME]/Layout/mechanism.js"></script>

   <script language="JavaScript">
  <!--
    [JS:CODE] 
   var IDBASKET='[FREEDOM_IDBASKET]';

  //-->
   </script>   



 </head>

<body class="freedom" >
<div id="col1" class="spaces">
[BLOCK SPACES]
<div class="space" onclick="expandToptree(this,'[sid]',document.getElementById('firstul'))" droppable="yes"   dropft="copy" onmouseover="overdragft(event,this)" onmouseout="outdragft(event,this)" onmouseup="insertinspace(event,this,'[sid]')"><img src="[sicon]"><span> [stitle]</span></div>
[ENDBLOCK SPACES]
<br style="clear:both">
<br>
<div class="space" style="display:none" onclick="globalcursor('move')" oncontextmenu="unglobalcursor()" ><img src="[IMG:gnome-fs-home.png]"><span> [TEXT:Private]</span></div>
<br>
<div class="space" id="itrash" onclick="viewFolder(event,'trash');" droppable="yes"   dropft="del" onmouseover="overdragft(event,this)" onmouseout="outdragft(event,this)" onmouseup="deleteinspace(event,this)"><img src="[imgtrash]" id="trashicon"><span> [TEXT:Trash]</span></div>
</div>

<div id="rule2" class="rule2"><table cellspacing="0"><tr ><td ><img  class="button" src="[IMG:b_minileft.png]"></td></tr></table></div>
<div id="col2" class="col2">
<div id="folders" class="folders">
<ul class="firstul" id="firstul"><table style="width:100%;height:80%"><tr><td align="center"><img style="width:48px" onload="if (isIE) correctOnePNG(this)" src="[IMG:b_init.png]"></tr></td></table></ul>
</div>
<div id="searches" class="searches">
<input id="ikey" title="[TEXT:input file name to search]" size="12" type="text" onkeypress="if (trackCR(event)) viewSearch(event,this.value);"></input><input type="image" class="button" title="[TEXT:Send searches]" onclick="viewSearch(event,document.getElementById('ikey').value);" align="absbottom" style="width:22px;border:none;background-color:transparent" src="[IMG:search.png]">
</div>
<div id="tabclip"  ondblclick="if (REFRESH) refreshClipBoard(CLIPCID,document.getElementById('clipboard'))" class="tabclip">
  <span  title="[TEXT:alternate between private space and document basket]" onclick="altern_basket_private(event,this)" ondblclick="expandPersoTree('[persofldid]',document.getElementById('secondul'),true)"><img src="[IMG:treeview.gif]"></span>
  <span  title="[TEXT:private space]"  style="display:none" id="tab_privatespace" onclick="expandPersoTree('[persofldid]',document.getElementById('secondul'))" ondblclick="expandPersoTree('[persofldid]',document.getElementById('secondul'),true)"><img src="[IMG:treeview.gif]"></span>
  <span title="[TEXT:my locked documents]" style="display:none"  onclick="clipviewornot(event,true);displayClipboard('lock',document.getElementById('clipboard'))"><img  src="[IMG:ylock.png]"></span>
  <span style="display:none" title="[TEXT:my documents to read]"><img src="[IMG:button-yellow.png]"></span>
  <span title="[TEXT:my basket]" style="display:none" id="tab_basket" onclick="clipviewornot(event,true);displayClipboard(IDBASKET,document.getElementById('clipboard'))"><img src="[IMG:iconview.gif]"></span>
  <span style="float:left;border:none;background-color:transparent" title="[TEXT:display/undisplay]" onclick="clipviewornot(event)"><img id="imgclipbutton" src="[IMG:b_down.png]"></span></div>
<div id="secondview"class="folders">
<div id="clipboard" style="display:none" onmouseup="insertinclipboard(event,this,CLIPCID)" >
<table style="width:100%;height:100%"><tr><td align="center"><img style="width:48px" onload="if (isIE) correctOnePNG(this)" src="[IMG:b_init.png]"></tr></td></table>
</div>
<ul class="firstul" id="secondul"><table style="width:70%;"><tr><td align="center"><img style="width:48px" onload="if (isIE) correctOnePNG(this)" src="[IMG:b_init.png]"></tr></td></table></ul>
</div>
</div>
</div>

<div id="col3" class="col3">
<div id="fldlist" class="fldlist" droppable="yes" dropft="move" onmouseup="insertinclipboard(event,this,CFLDID,'list')" onmouseover="overdragft(event,this)" onmouseout="outdragft(event,this)"><table style="width:100%;height:100%"><tr><td align="center"><img style="width:48px" onload="if (isIE) correctOnePNG(this)" src="[IMG:b_init.png]"></tr></td></table>
</div>
<div id="resume" class="resume">
<iframe id="iresume" name="nresume" src="[CORE_STANDURL]app=CORE&action=BLANK" style="margin:0px;width:100%;height:99%"></iframe>
</div></div>

</body></html>
