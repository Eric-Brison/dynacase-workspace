[CSS:REF]
<link href="[APPNAME]/Layout/system.css" type="text/css" rel="stylesheet">
<link href="[APPNAME]/Layout/default.css" type="text/css" rel="stylesheet"   title="Preferee">
<link href="black.css" type="text/css" rel="alternate stylesheet"  title="Noir">
<link href="white.css" type="text/css" rel="alternate stylesheet"  title="Blanc">


