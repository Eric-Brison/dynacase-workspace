[JS:REF]

<script type="text/javascript" src="[APPNAME]/Layout/pngfix.js"></script>
<script type="text/javascript" src="[APPNAME]/Layout/displayws.js"></script>
<script type="text/javascript" src="[APPNAME]/Layout/mechanism.js"></script>
<script type="text/javascript" src="[APPNAME]/Layout/popupdoc.js"></script>
   <script language="JavaScript">
  <!--
    [JS:CODE] 
   var idbasket='[FREEDOM_IDBASKET]';
var fdl_vd2size=parseInt('[FDL_VD2SIZE]');
var fdl_hd2size=parseInt('[FDL_HD2SIZE]');


function expandtree(oimg,id,ulid,adddocid,padddocid,addft,reset) {
  
  var r=viewfoldertree(oimg,id,document.getElementById(ulid),adddocid,padddocid,addft,reset);
  if (r==1) {
    oimg.src='[IMG:b_down.png]'; 
  } else if (r==0) {
    oimg.src='[IMG:b_right.png]';
  } else if (r==2) {
    oimg.src='[IMG:b_wait.png]';
  }
  if (isIE) correctOnePNG(oimg);
}
function expandToptree(o,id,ulid) {

  var ldiv=o.parentNode.getElementsByTagName("div");
  for (var i=0;i<ldiv.length;i++) {
    if (ldiv[i].className=='spaceselect') ldiv[i].className='space';
  }
  o.className="spaceselect";
  CURSPACE=o;
  var r=viewfoldertree(null,id,ulid);
  
  ulid.innerHTML='<table style="width:100%;height:80%"><tr><td align="center"><img style="width:48px" onload="if (isIE) correctOnePNG(this)" src="[IMG:b_wait.png]"></tr></td></table>';
}
function clipboardWait(o) {
  if (o) o.innerHTML='<table style="width:100%;height:100%"><tr><td align="center"><img style="width:48px"  src="[IMG:b_wait.gif]"></tr></td></table>';
}
function clipboardWaitEnd(o) {
  if (o) o.innerHTML='<table style="width:100%;height:100%"><tr><td align="center"><img style="width:48px" onload="if (isIE) correctOnePNG(this)" src="[IMG:b_waitend.png]"></tr></td></table>';
}

   function endexpandtree(o,c) {
     if (o) {
       if (c==0) {
	 o.src='[IMG:none.png]'; 
	 o.style.visibility='hidden';
	 if (isIE) correctOnePNG(o);
       }
       else {
	 o.src='[IMG:b_down.png]'; 
	 o.style.visibility='visible';
       }
     }
   }
  //-->
   </script>   



 </head>

<body class="freedom">
<div id="col1" class="spaces">
[BLOCK SPACES]
<div class="space" onclick="expandToptree(this,'[sid]',document.getElementById('firstul'))" droppable="yes"   dropft="add" onmouseover="overdragft(event,this)" onmouseout="outdragft(event,this)" onmouseup="insertinspace(event,this,'[sid]')"><img src="[sicon]"><span> [stitle]</span></div>
[ENDBLOCK SPACES]
<br style="clear:both">
<br>
<div class="space"  ><img src="[IMG:gnome-fs-home.png]"><span> [TEXT:Private]</span></div>
<br>
<div class="space" id="itrash" onclick="viewFolder(event,'trash');" droppable="yes"   dropft="del" onmouseover="overdragft(event,this)" onmouseout="outdragft(event,this)" onmouseup="deleteinspace(event,this)"><img src="[imgtrash]"><span> [TEXT:Trash]</span></div>
</div>

<div id="rule2" class="rule2"><table cellspacing="0"><tr ><td ><img  class="button" src="b_minileft.png"></td></tr></table></div>
<div id="col2" class="col2">
<div id="folders" class="folders">
<ul id="firstul"><table style="width:100%;height:80%"><tr><td align="center"><img style="width:48px" onload="if (isIE) correctOnePNG(this)" src="[IMG:b_init.png]"></tr></td></table></ul>
</div>
<div id="searches" class="searches">
<input title="saisissez les mots &agrave; rechercher" size="12" type="text"></input> <a href="" title="lancer la recherche"><img class="button" width="22px" src="[IMG:search.png]"></a>
</div>
<div id="tabclip"  ondblclick="if (REFRESH) refreshClipBoard(CLIPCID,document.getElementById('clipboard'))" class="tabclip">
  <span title="[TEXT:my affected documents]"><img src="[IMG:button-blue.png]"></span>
  <span title="[TEXT:my locked documents]" onclick="refreshClipBoard('lock',document.getElementById('clipboard'))"><img  src="[IMG:ylock.png]"></span>
  <span title="[TEXT:my ... documents]"><img src="[IMG:button-yellow.png]"></span>
  <span title="[TEXT:my basket]" onclick="refreshClipBoard(idbasket,document.getElementById('clipboard'))"><img src="[IMG:button-green.png]"></span></div>
<div id="clipboard" class="clipboard"    onmouseup="insertinclipboard(event,this,CLIPCID)" >
<table style="width:100%;height:100%"><tr><td align="center"><img style="width:48px" onload="if (isIE) correctOnePNG(this)" src="[IMG:b_init.png]"></tr></td></table>
</div>
</div>
</div>

<div id="col3" class="col3">
<div id="fldlist" class="fldlist" droppable="yes" dropft="move" onmouseup="insertinclipboard(event,this,CFLDID,'list')" onmouseover="overdragft(event,this)" onmouseout="outdragft(event,this)"><table style="width:100%;height:100%"><tr><td align="center"><img style="width:48px" onload="if (isIE) correctOnePNG(this)" src="[IMG:b_init.png]"></tr></td></table>
</div>
<div id="resume" class="resume">
<iframe id="iresume" style="width:99%;height:99%"></iframe>
</div></div>
<div   id="popupzone" style="position:absolute;"></div>
</body></html>
